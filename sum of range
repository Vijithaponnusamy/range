import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n =s.nextInt();
        int m =s.nextInt();
        
        int sum = 0;
        for (int i =n;i<=m;i++){
            sum+=i;
            
        }
        System.out.println("The sum of Range value from " +n+ " to " +m+ " is " +sum+".0");
        
    }
}
